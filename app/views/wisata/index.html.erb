<p id="notice"><%= notice %></p>

<h1>Listing Wisata</h1>

<table>
  <thead>
    <tr>
      <th>Nama</th>
      <th>Harga</th>
      <th>Info</th>
      <th>Foto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wisata.each do |wisatum| %>
      <tr>
        <td><%= wisatum.nama %></td>
        <td><%= wisatum.harga %></td>
        <td><%= wisatum.info %></td>
        <td><%= wisatum.foto %></td>
        <td><%= link_to 'Show', wisatum %></td>
        <td><%= link_to 'Edit', edit_wisatum_path(wisatum) %></td>
        <td><%= link_to 'Destroy', wisatum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wisatum', new_wisatum_path %>
