<p id="notice"><%= notice %></p>

<h1>Daftar Wisata</h1>
<div class="table-responsive table-bordered">
  <table class="table">
    <thead>
    <tr>
      <th>Nama</th>
      <th>Harga</th>
      <th>Info</th>
      <th>Foto</th>
      <th colspan="3"></th>
    </tr>
  </thead>
    
  <tbody>
    <% @wisata.each do |wisatum| %>
      <tr>
        <td><%= wisatum.nama %></td>
        <td><%= wisatum.harga %></td>
        <td><%= wisatum.info %></td>
        <td><%= wisatum.foto %></td>
        <td><%= link_to 'Show', wisatum %></td>
        <td><%= link_to 'Edit', edit_wisatum_path(wisatum) %></td>
        <td><%= link_to 'Destroy', wisatum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
  </table>
</div>
<br>

<a href="../wisata/new" class="btn btn-primary" role="button">Wisata Baru</a>

